// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          String @id @default(uuid())
  nome        String
  email       String
  senha       String
  isAdmin     Boolean
  created_at DateTime @default(now())
}
